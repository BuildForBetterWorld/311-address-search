{
	"manifest_version": 2,
	"name": "311 for Addresses",
	"version": "1.0",
	"background": {
		"scripts": ["popup.js"],
		"persistent": false
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["content.js"]
		}
	],
	"browser_action": {
		"default_title": "311",
		"default_icon": "icon.png"
	},
	"permissions": [
	"activeTab"
	]
}