{
	"manifest_version": 2,
	"name": "311 for Addresses",
	"version": "1.0",
	"content_scripts": [
		{
			//"run_at": "document_end",
			"matches": [
				"<all_urls>"
			],
			"js": [
				"jquery-1.12.1.min.js"
				//"content.js"
			]
		}
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_title": "311",
		"default_icon": "icon.png"
	},
	"permissions": [
	"activeTab"
	]
}